{
  "name": "@datalayer/lexical-loro",
  "version": "0.6.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development concurrently \"npm:server:ws\" \"vite  --host --port 3000\"",
    "build:tsc": "tsc",
    "build:dev": "vite build",
    "build:prod": "vite build --mode production",
    "server:ws": "cross-env HOST=localhost PORT=1234 YPERSISTENCE=./yjs-wss-db npx y-websocket",
    "server:ws:min": "fkill -s :1234 && tsx src/servers/yjs/ws-min/server.ts",
    "server:ws:full": "fkill -s :1234 && YPERSISTENCE=./data-server node src/servers/yjs/ws-full/server.js",
    "preview": "vite preview"
  },
  "dependencies": {
    "@excalidraw/excalidraw": "^0.18.0",
    "@lexical/clipboard": "0.35.0",
    "@lexical/code": "0.35.0",
    "@lexical/code-shiki": "0.35.0",
    "@lexical/file": "0.35.0",
    "@lexical/hashtag": "0.35.0",
    "@lexical/link": "0.35.0",
    "@lexical/list": "0.35.0",
    "@lexical/mark": "0.35.0",
    "@lexical/overflow": "0.35.0",
    "@lexical/plain-text": "0.35.0",
    "@lexical/react": "0.35.0",
    "@lexical/rich-text": "0.35.0",
    "@lexical/selection": "0.35.0",
    "@lexical/table": "0.35.0",
    "@lexical/utils": "0.35.0",
    "date-fns": "^4.1.0",
    "katex": "^0.16.10",
    "lexical": "0.35.0",
    "lodash-es": "^4.17.21",
    "prettier": "^2.8.8",
    "react": "^18.2.0",
    "react-day-picker": "^9.7.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^3.1.4",
    "y-websocket": "^1.5.4",
    "yjs": ">=13.5.42"
  },
  "devDependencies": {
    "@babel/plugin-transform-flow-strip-types": "^7.24.7",
    "@babel/preset-react": "^7.24.7",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@types/lodash-es": "^4.14.182",
    "@vitejs/plugin-react": "^4.2.1",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "rollup-plugin-copy": "^3.5.0",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
